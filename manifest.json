{
  "manifest_version": 2,
  "name": "COVID-19 Tracker",
  "version": "1.0",
  "description": "This Chrome Extension is a COVID-19 Tracker",
  "content_scripts": [
    {
      "matches": ["http://*/*"],
      "js": ["/js/main.js"],
      "css": ["/css/styles.css"]
    }
  ],
  "browser_action": {
    "default_title": "COVID-19 Tracker",
    "default_popup": "index.html",
    "default_icon": {
      "128": "images/128x128.png"
    }
  }
}
